{
  "buildCommand": "npm install --include=dev && npm run build",
  "outputDirectory": "dist/public",
  "installCommand": "npm install --include=dev",
  "functions": {
    "dist/index.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/dist/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/public/$1"
    }
  ]
}